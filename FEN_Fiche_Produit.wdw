  procedures :
   -
     name : ChargerPriceBreaks
     procedure_id : 1726438280171707061
     type_code : 14
     code : |1+
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      //ChargerPriceBreaks (<idProduitLocal> est chaîne)
      //
      // Paramètres :
      //	idProduitLocal (chaîne ANSI) : <indiquez ici le rôle de idProduitLocal>
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // <Indiquez ici un exemple d'utilisation>
      PROCÉDURE ChargerPriceBreaks(idProduitLocal est un entier sur 8 octets)
      
      // Recherche du pricing pour le produit
      //HLitRecherche()
      SI HLitRecherchePremier(Pricing, idProduct, idProduitLocal) ALORS
      		// Charger les PriceBreaks associés à ce pricing
      
      	SI HLitRecherchePremier(PriceBreak, idPricing, Pricing.idPricing) ALORS
      		// Remplir la table des PriceBreaks
      		TANTQUE HTrouve(PriceBreak)
      			TABLE_Pricing.AjouteLigne(PriceBreak.priceBreakQuantity, PriceBreak.price)
      			HLitSuivant(PriceBreak)
      		FIN
      
      	SINON
      		Info("Aucun PriceBreak trouvé pour ce produit.")
      	FIN
      SINON
      	Info("Aucun Pricing trouvé pour ce produit.")
      FIN
      
     type : 458752
   -
     name : CalculerPrixTotal
     procedure_id : 1726438451970635650
     type_code : 14
     code : |1+
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      // CalculerPrixTotal ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // <Indiquez ici un exemple d'utilisation>
      //
      PROCÉDURE CalculerPrixTotal()
      qteCommandee	est un entier	= SAI_QuantitéCommande	// Champ de saisie pour la quantité
      prixTotal		est un réel		= 0
      
      // Récupérer le nombre de lignes dans la table TABLE_Pricing
      nbLignes		est un entier	= TableOccurrence(TABLE_Pricing)
      // Déclarer des variables pour déterminer la tranche de prix la plus adaptée
      meilleurSeuil	est un entier	= 0
      prixApplicable	est un réel		= 0
      
      // Parcourir chaque ligne de la table TABLE_Pricing
      POUR i = 1 À nbLignes
      	// Récupérer le seuil (priceBreakQuantity) de la ligne i
      	seuil est un entier = ChaîneVersNumérique(TABLE_Pricing[i].COL_PriceBreak)
      	// Vérifier si la quantité commandée est supérieure ou égale au seuil
      	SI qteCommandee >= seuil ALORS
      		// Si ce seuil est supérieur au meilleur seuil trouvé jusqu'ici, le retenir
      		SI seuil > meilleurSeuil ALORS
      			meilleurSeuil	= seuil
      			prixApplicable	= ChaîneVersNumérique(TABLE_Pricing[i].COL_Price)
      		FIN
      	FIN
      FIN
      
      // Calculer le prix total si une tranche applicable a été trouvée
      SI meilleurSeuil > 0 ALORS
      	prixTotal = qteCommandee * prixApplicable
      SINON
      	prixTotal = 0
      FIN
      
      // Afficher le prix total dans le champ/label SAI_PrixTotal
      SAI_PrixTotal = prixTotal
      
      
      
      
     type : 458752
